{
    "dockerFile": "Dockerfile",
    "extensions": [
        "akamud.vscode-theme-onelight",
        "denoland.vscode-deno",
        "eamodio.gitlens",
        "github.github-vscode-theme",
        "matklad.rust-analyzer",
        "ms-vscode.theme-1337",
        "tamasfe.even-better-toml",
        "Tyriar.sort-lines",
        "usernamehw.errorlens"
    ],
    "settings": {
        "workbench.colorCustomizations": {
            "editorInlayHint.foreground": "#898a60",
            "editorInlayHint.parameterBackground": "#60303010",
            "editorInlayHint.parameterForeground": "#968383",
            "editorInlayHint.typeBackground": "#30603010",
            "editorInlayHint.typeForeground": "#808080"
        },
        "workbench.colorTheme": "1337",
        "editor.tokenColorCustomizations": {
            "textMateRules": [
                {
                    "scope": "punctuation.definition.comment",
                    "settings": {
                        "foreground": "#0A44",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ],
            "[Atom One Dark]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#FFF"
                        }
                    }
                ]
            },
            "[1337]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#FFF"
                        }
                    }
                ]
            },
            "[GitHub Dark Default]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#FFF"
                        }
                    }
                ]
            },
            "[GitHub Light Default]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#000"
                        }
                    }
                ]
            },
            "[Atom One Light]": {
                "textMateRules": [
                    {
                        "scope": "comment",
                        "settings": {
                            "foreground": "#000"
                        }
                    }
                ]
            }
        }
    }
}
